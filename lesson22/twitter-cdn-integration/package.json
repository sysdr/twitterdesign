{
  "name": "twitter-cdn-integration",
  "version": "1.0.0",
  "description": "CDN Integration for Twitter Clone",
  "main": "src/index.ts",
  "scripts": {
    "dev": "concurrently \"npm run dev:client\" \"npm run dev:server\"",
    "dev:client": "vite",
    "dev:server": "ts-node-dev --respawn --transpile-only src/server/index.ts",
    "build": "vite build && tsc --project tsconfig.server.json",
    "test": "jest",
    "test:integration": "jest --config jest.integration.config.js",
    "lint": "eslint src --ext .ts,.tsx",
    "start": "node dist/server/index.js"
  },
  "dependencies": {
    "aws-sdk": "^2.1329.0",
    "axios": "^1.3.0",
    "cloudflare": "^2.9.1",
    "compression": "^1.7.4",
    "cors": "^2.8.5",
    "crypto": "^1.0.1",
    "express": "^4.18.0",
    "helmet": "^6.0.0",
    "ioredis": "^5.3.0",
    "node-cron": "^3.0.2",
    "react": "^18.2.0",
    "react-dom": "^18.2.0",
    "react-router-dom": "^6.8.0",
    "redis": "^4.6.0",
    "uuid": "^9.0.0",
    "ws": "^8.13.0"
  },
  "devDependencies": {
    "@types/compression": "^1.7.0",
    "@types/cors": "^2.8.0",
    "@types/express": "^4.17.0",
    "@types/jest": "^29.4.0",
    "@types/node": "^18.14.0",
    "@types/react": "^18.0.0",
    "@types/react-dom": "^18.0.0",
    "@types/supertest": "^6.0.3",
    "@types/uuid": "^9.0.0",
    "@types/ws": "^8.5.0",
    "@typescript-eslint/eslint-plugin": "^5.52.0",
    "@vitejs/plugin-react": "^3.1.0",
    "concurrently": "^7.6.0",
    "eslint": "^8.34.0",
    "ioredis-mock": "^8.9.0",
    "jest": "^29.4.0",
    "supertest": "^7.1.4",
    "ts-jest": "^29.0.0",
    "ts-node-dev": "^2.0.0",
    "typescript": "^4.9.0",
    "vite": "^4.1.0"
  }
}
